<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<?xml-stylesheet href="chrome://akahuku/content/options.css" type="text/css"?>

<dialog class="dialog"
        id="akahuku_preferences_dialog"
        title="赤福 設定"
        persist="screenX screenY"
        buttons="accept,cancel,extra1"
        buttonlabelextra1="適用"
        onload="AkahukuOptions.init ();"
        ondialogaccept="return AkahukuOptions.term ()"
        orient="vertical"
        flex="1"
        xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  
  <script type="application/x-javascript"
          src="chrome://akahuku/content/version.js" />
  
  <script type="application/x-javascript"
          src="chrome://akahuku/content/mod/arAkahukuLocationInfo.js" />
  <script type="application/x-javascript"
          src="chrome://akahuku/content/mod/arAkahukuConverter.js" />
  <script type="application/x-javascript"
          src="chrome://akahuku/content/mod/arAkahukuFile.js" />
  <script type="application/x-javascript"
          src="chrome://akahuku/content/mod/arAkahukuFileName.js" />
  <script type="application/x-javascript"
          src="chrome://akahuku/content/mod/arAkahukuServerName.js" />
  <script type="application/x-javascript"
          src="chrome://akahuku/content/mod/arAkahukuTitle.js" />
  <script type="application/x-javascript"
          src="chrome://akahuku/content/mod/arAkahukuCompat.js" />
  
  <script type="application/x-javascript"
          src="chrome://akahuku/content/options.js" />
  
  <box orient="horizontal" align="center">
    <checkbox id="all" checked="false" label="全機能" />
    <spacer flex="1" />
    <label id="link" class="text-link" value="赤福:非公式 Firefox 版"
           onclick="AkahukuOptions.openWebsite ();" />
    <label id="version" value="Ver." />
  </box>
  
  <box orient="horizontal" flex="1">
    <listbox id="category" style="width: 12em;">
      <listitem id="anchor_title" label="タイトル" />
      <listitem id="anchor_scroll" label="スクロール" />
      <listitem id="anchor_banner" label="広告" />
      <listitem id="anchor_wheel" label="ホイール" />
      <listitem id="anchor_tab" label="タブ" />
      <listitem id="anchor_menu" label="メニュー/引用" />
      <listitem id="anchor_savemht" label="MHT で保存" />
      <listitem id="anchor_filename_convert" label="ファイル名変換" />
      <listitem id="anchor_saveimage1" label="画像を保存1" />
      <listitem id="anchor_saveimage2" label="画像を保存2" />
      <listitem id="anchor_mail_comment" label="メール／コメント欄" />
      <listitem id="anchor_form" label="フォーム" />
      <listitem id="anchor_reload" label="続きを読む" />
      <listitem id="anchor_throp" label="スレ操作パネル" />
      <listitem id="anchor_link" label="リンク" />
      <listitem id="anchor_thread" label="スレ" />
      <listitem id="anchor_popupquote" label="引用のポップアップ" />
      <listitem id="anchor_hidetroll" label="芝刈り" />
      <listitem id="anchor_res" label="レス一般" />
      <listitem id="anchor_autolink" label="オートリンク" />
      <listitem id="anchor_p2p" label="P2P" />
      <listitem id="anchor_catalog1" label="カタログ1" />
      <listitem id="anchor_catalog2" label="カタログ2" />
      <listitem id="anchor_sidebar" label="サイドバー" />
      <listitem id="anchor_board" label="板" />
      <listitem id="anchor_external" label="外部の板" />
      <listitem id="anchor_bloomer" label="ブルマ女将" />
      <listitem id="anchor_sound" label="音" />
      <listitem id="anchor_other" label="その他" />
      <listitem id="anchor_format" label="フォーマット" />
    </listbox>
    
    <deck id="tabbox" flex="1">
      <box flex="1" id="tab_title" orient="vertical">
        <groupbox orient="vertical">
          <caption>
            <checkbox id="title" checked="false" label="タイトルを修正"
                      oncommand="AkahukuOptions.checkTitle ();" />
          </caption>
          <radiogroup id="title_type" orient="horizontal"
                      align="center"
                      oncommand="AkahukuOptions.checkTitleType ();">
            <radio value="simple" label="簡易設定" />
            <radio value="expert" label="詳細設定" />
            <spacer flex="1" />
            <button id="title_format_init" label="初期化"
                    oncommand="AkahukuOptions.initTitleFormat ();" />
          </radiogroup>
          <deck id="title_type_deck">
            <box id="title_type_deck_simple" orient="vertical" flex="1">
              <checkbox id="title_comment" checked="false"
                        label="スレ本文の先頭" />
              <checkbox id="title_mode" checked="false"
                        label="[ページ n]、[返信] 等" />
              <checkbox id="title_thread_info" checked="false"
                        label="スレの消滅情報" />
              <box orient="horizontal">
                <checkbox id="title_incoming_reply" checked="false"
                          class="sp" tooltiptext="(デフォルト：無効) 新着レス数をタイトルの頭に (1) の用に付ける。新着レス数は他タブでのカタログやスレ等の観覧により判定されリアルタイムで更新される。"
                          label="スレの新着レス数" />
              </box>
            </box>
            <box id="title_type_deck_expert" orient="vertical" flex="1">
              <textbox id="title_format" multiline="true" rows="3"
                       oninput="AkahukuOptions.changeFormat ('title_format');" />
              <textbox id="title_format_sample" value="" disabled="true" />
            </box>
          </deck>
        </groupbox>
        <box orient="horizontal" align="center">
          <label id="title_comment_length_label1"
                 control="title_comment_length"
                 value="スレ本文の先頭の長さ" />
          <textbox id="title_comment_length" size="6" />
          <menulist id="title_comment_length_type"
                    tooltiptext="(デフォルト:文字) 長さの単位">
            <menupopup id="title_comment_lengthtype_menu">
              <menuitem value="0" label="文字" />
              <menuitem value="1" label="バイト" />
            </menupopup>
          </menulist>
          <label id="title_comment_length_label2"
                 control="title_comment_length"
                 value="まで (&amp;message;と&amp;message2;の長さ)" />
        </box>
        <box class="sub" orient="vertical">
          <checkbox id="title_comment_multiline" checked="false"
                    class="sp" tooltiptext="(デフォルト：無効) 最初の行だけでなく、複数行に渡るスレ本文を連結してから「スレ本文の先頭」を抽出する。"
                    label="複数行から" />
        </box>
        <checkbox id="comment_fixup" checked="false"
                  label="本文なしを適当に修正する" />
        <checkbox id="subtitle" checked="false"
                  label="レスの一部をサブタイトルにする" />
      </box>
      <box flex="1" id="tab_scroll" orient="vertical">
        <checkbox id="scroll_lock" checked="false"
                  label="リロード時に位置のズレを直す"
                  oncommand="AkahukuOptions.checkScrollLock ();" />
        <box class="sub" orient="vertical">
          <checkbox id="scroll_lock_reply" checked="false"
                    label="レス送信モードのみ" />
        </box>
        <checkbox id="scroll_gotop" checked="false"
                  label="0〜10 ページのリロード時に先頭へ" />
        <checkbox id="scroll_gocurrent_reload" checked="false"
                  label="レス送信モードでリロード時に最新位置へ"
                  oncommand="AkahukuOptions.checkScrollGoCurrent ();" />
        <box class="sub" orient="vertical">
          <checkbox id="scroll_gocurrent_rule" checked="false"
                    label="区切り"
                    oncommand="AkahukuOptions.checkScrollGoCurrentRule ();" />
          <box class="sub" orient="vertical">
            <checkbox id="scroll_gocurrent_rule_zeroheight"
                      checked="false"
                      label="ズレないようにする" />
            <checkbox id="scroll_gocurrent_rule_random" checked="false"
                      label="ランダム" />
          </box>
        </box>
      </box>
      <box flex="1" id="tab_banner" orient="vertical">
        <groupbox orient="vertical">
          <caption>
            <checkbox id="delbanner" checked="false"
                      label="広告を隠す"
                      oncommand="AkahukuOptions.checkDelbanner ();" />
          </caption>
          <groupbox orient="vertical">
            <caption>
              <label id="delbanner_contentpolicy_group_label"
                     value="外部サイトをブロック"/>
            </caption>
            <box orient="horizontal">
              <checkbox id="delbanner_image" checked="false"
                        label="バナー広告"
                        oncommand="AkahukuOptions.checkDelbannerImage ();"/>
              <checkbox id="delbanner_image_404" checked="false"
                        label="404 ページの広告"
                        oncommand="AkahukuOptions.checkDelbannerImage ();"/>
            </box>
            <tooltip id="delbanner_sites_tooltip"
                     orient="vertical">
              <label value="ブロックリスト:"/>
              <box class="sub" orient="vertical">
                <label value="ふたばのページからの読み込みをブロックさせたいURLやホスト名を空白区切りで入力する"/>
                <label value="(ホスト名指定は後方一致、URL指定は完全一致だがワイルドカード(*)が使える)"/>
                <label value="例: http://mushoku.com/banner.jpg munen.org "/>
                <label value="例2: http://*.2chan.net/bin/radif.htm http://*.hoge.jp/fuga?*"/>
                <label value="例3: http*://mushoku.com/banner.jpg //munen.org/ad.htm"/>
              </box>
            </tooltip>
            <box class="sub" orient="vertical">
              <box orient="horizontal" align="center">
                <label id="delbanner_sites_image_label"
                       control="delbanner_sites_image"
                       tooltip="delbanner_sites_tooltip"
                       value="画像ブロックリスト"/>
                <textbox id="delbanner_sites_image" flex="1"
                         tooltip="delbanner_sites_tooltip"/>
              </box>
              <box orient="horizontal" align="center">
                <label id="delbanner_sites_iframe_label"
                       control="delbanner_sites_iframe"
                       tooltip="delbanner_sites_tooltip"
                       value="IFRAMEブロックリスト"/>
                <textbox id="delbanner_sites_iframe" flex="1"
                         tooltip="delbanner_sites_tooltip"/>
              </box>
            </box>
            <checkbox id="delbanner_flash" checked="false"
                      label="Flash 広告 (埋込オブジェクト)"
                      oncommand="AkahukuOptions.checkDelbannerFlash ();"/>
            <box class="sub" orient="vertical">
              <box orient="horizontal" align="center">
                <label id="delbanner_sites_object_label"
                       control="delbanner_sites_object"
                       tooltip="delbanner_sites_tooltip"
                       value="OBJECTブロックリスト"/>
                <textbox id="delbanner_sites_object" flex="1"
                         tooltip="delbanner_sites_tooltip"/>
              </box>
            </box>
          </groupbox>
          <checkbox id="delbanner_text" checked="false"
                    label="テキスト広告" />
          <checkbox id="delbanner_movetailad" checked="false"
                    label="末尾の広告を横に置く (ある程度の横幅が必要です)"
                    oncommand="AkahukuOptions.checkDelbannerTailAd ();" />
          <box class="sub" orient="vertical">
            <checkbox id="delbanner_movetailad_all" checked="false"
                      class="sp" tooltiptext="(デフォルト：無効) Amazon以外のバナーも全て横に置く"
                      label="全て横に" />
          </box>
          <checkbox id="delbanner_monotonize" checked="false"
                    class="sp" tooltiptext="(デフォルト：無効) 広告の見た目を CSS でモノトーンに色変換して表示する。CPU や GPU への負荷がかかるため注意。Firefox 35 以降が必要。"
                    label="モノトーン化" />
        </groupbox>
      </box>
      <box flex="1" id="tab_wheel" orient="vertical">
        <groupbox orient="vertical">
          <caption orient="horizontal" align="center">
            <checkbox id="wheel_reload" checked="false"
                      label="ページ末尾のホイール"
                      oncommand="AkahukuOptions.checkWheelReload ();" />
            <textbox id="wheel_reload_threshold" size="6" />
            <label id="wheel_reload_threshold_label"
                   control="wheel_reload_threshold"
                   value="回操作でリロード" />
          </caption>
          
          <checkbox id="wheel_reload_0" checked="false"
                    label="通常モード 0 ページ" />
          <checkbox id="wheel_reload_1" checked="false"
                    label="通常モード 1 ページ〜 " />
          <checkbox id="wheel_reload_loop" checked="false"
                    label="通常モードの上下でループする" />
          
          <checkbox id="wheel_reload_reply" checked="false"
                    label="レス送信モード" />
          <box class="sub" orient="vertical">
            <checkbox id="wheel_reload_reply_sync" checked="false"
                      label="同期する" />
          </box>
          <checkbox id="wheel_reload_catalog" checked="false"
                    label="カタログモード"
                    oncommand="AkahukuOptions.checkWheelReloadCatalog ();" />
          <box class="sub" orient="vertical">
            <checkbox id="wheel_reload_catalog_up" checked="false"
                      class="sp" tooltiptext="(デフォルト：無効) 上端での上スクロールでもリロードする"
                      label="ページ上端でも" />
          </box>
          <checkbox id="wheel_reload_all" checked="false"
                    label="全てのページ" />
        </groupbox>
      </box>
      <box flex="1" id="tab_tab" orient="vertical">
        <groupbox orient="horizontal">
          <caption>
            <checkbox id="tab_sort" checked="false" label="ソート"
                      oncommand="AkahukuOptions.checkTabSort ();" />
          </caption>
          <box orient="vertical" flex="1">
            <checkbox id="tab_sort_thread" checked="false" label="スレをソート"
                      oncommand="AkahukuOptions.checkTabSortChild ();"  />
            <checkbox id="tab_sort_all" checked="false" label="全てソート"
                      oncommand="AkahukuOptions.checkTabSortChild ();"  />
            <box class="sub" orient="vertical">
              <box orient="horizontal" align="center">
                <label id="tab_sort_order_1_label"
                       control="tab_sort_order_1"
                       value="1: " />
                <menulist id="tab_sort_order_1"
                          oncommand="AkahukuOptions.onChangeTabSortOrder (1);">
                  <menupopup id="tab_sort_order_1_menu">
                    <menuitem label="通常モード" value="normal" />
                    <menuitem label="レス送信モード" value="reply" />
                    <menuitem label="カタログモード" value="catalog" />
                    <menuitem label="その他" value="other" />
                  </menupopup>
                </menulist>
              </box>
              <box orient="horizontal" align="center">
                <label id="tab_sort_order_2_label"
                       control="tab_sort_order_2"
                       value="2: " />
                <menulist id="tab_sort_order_2"
                          oncommand="AkahukuOptions.onChangeTabSortOrder (2);">
                  <menupopup id="tab_sort_order_2_menu">
                    <menuitem label="通常モード" value="normal" />
                    <menuitem label="レス送信モード" value="reply" />
                    <menuitem label="カタログモード" value="catalog" />
                    <menuitem label="その他" value="other" />
                  </menupopup>
                </menulist>
              </box>
              <box orient="horizontal" align="center">
                <label id="tab_sort_order_3_label"
                       control="tab_sort_order_3"
                       value="3: " />
                <menulist id="tab_sort_order_3"
                          oncommand="AkahukuOptions.onChangeTabSortOrder (3);">
                  <menupopup id="tab_sort_order_3_menu">
                    <menuitem label="通常モード" value="normal" />
                    <menuitem label="レス送信モード" value="reply" />
                    <menuitem label="カタログモード" value="catalog" />
                    <menuitem label="その他" value="other" />
                  </menupopup>
                </menulist>
              </box>
              <box orient="horizontal" align="center">
                <label id="tab_sort_order_4_label"
                       control="tab_sort_order_4"
                       value="4: " />
                <menulist id="tab_sort_order_4"
                          oncommand="AkahukuOptions.onChangeTabSortOrder (4);">
                  <menupopup id="tab_sort_order_4_menu">
                    <menuitem label="通常モード" value="normal" />
                    <menuitem label="レス送信モード" value="reply" />
                    <menuitem label="カタログモード" value="catalog" />
                    <menuitem label="その他" value="other" />
                  </menupopup>
                </menulist>
              </box>
              <checkbox id="tab_sort_invert_thread" checked="false"
                        label="スレの並びを反転" />
            </box>
          </box>
          <box orient="vertical" flex="1">
            <label id="tab_sort_board_order_label" value="板の順番" />
            <box orient="horizontal" flex="1">
              <listbox id="tab_sort_board_order_list" flex="1"
                       rows="6" seltype="multiple">
              </listbox>
              <box orient="vertical">
                <button id="tab_sort_board_order_moveup" label="上に"
                        oncommand="AkahukuOptions.onMoveupList ('tab_sort_board_order_list');" />
                <button id="tab_sort_board_order_movedown" label="下に"
                        oncommand="AkahukuOptions.onMovedownList ('tab_sort_board_order_list');" />
              </box>
            </box>
          </box>
        </groupbox>
        <groupbox orient="vertical">
          <caption>
            <checkbox id="tabicon" checked="false"
                      label="アイコンを変更する"
                      oncommand="AkahukuOptions.checkTabIcon ();" />
          </caption>
          <box orient="horizonaral" align="center">
            <checkbox id="tabicon_size" checked="false"
                      label="サイズを"
                      oncommand="AkahukuOptions.checkTabIconSize ();"/>
            <textbox id="tabicon_size_max" size="6" />
            <label id="tabicon_size_max_label"
                   control="tabicon_size_max" value="px にする" />
          </box>
          <checkbox id="tabicon_asfavicon" checked="false"
                    class="sp" tooltiptext="(デフォルト：無効) ブックマーク、履歴、サイトアイコンなど、タブ以外の全てに適用されるよう登録する"
                    label="favicon として登録する"/>
        </groupbox>
      </box>
      
      <box flex="1" id="tab_menu" orient="vertical">
        <groupbox orient="horizontal">
          <caption>
            <checkbox id="quickquote" checked="false" label="引用/コンテキストメニュー"
                      oncommand="AkahukuOptions.checkQuickQuote ();" />
          </caption>
          <box orient="vertical" flex="1">
            <checkbox id="quickquote_menu" label="メニュー"
                      oncommand="AkahukuOptions.checkQuickQuoteMenu ();" />
            <box class="sub" orient="vertical">
              <checkbox id="quickquote_menu_quote" checked="false"
                        label="引用" />
              <checkbox id="quickquote_menu_mail" checked="false"
                        label="メール欄へ" />
              <checkbox id="quickquote_menu_name" checked="false"
                        label="名前欄へ" />
              <checkbox id="quickquote_menu_comment" checked="false"
                        label="コメント欄へ" />
              <checkbox id="quickquote_menu_separator" checked="false"
                        label="区切り" />
              <checkbox id="quickquote_menu_copy" checked="false"
                        label="引用付きコピー" />
              <checkbox id="quickquote_menu_cont" checked="false"
                        label="- 連続" />
              <checkbox id="quickquote_menu_google_image" checked="false"
                        label="イメぐぐる" />
              <checkbox id="quickquote_menu_wikipedia" checked="false"
                        label="ウィキペドる" />
            </box>
          </box>
          <box orient="vertical" flex="1">
            <checkbox id="quickquote_number" checked="false"
                      label="番号をクリックで引用/「引用して書込み」のカスタマイズ"
                      oncommand="AkahukuOptions.checkQuickQuoteNumber ();" />
            <box class="sub" orient="vertical">
              <menulist id="quickquote_number_type">
                <menupopup id="quickquote_number_type_menu">
                  <menuitem value="0" label="番号" />
                  <menuitem value="1" label="No. + 番号" />
                  <menuitem value="2" label="メッセージ" />
                  <menuitem value="3" label="メッセージ (非引用部分のみ)" />
                </menupopup>
              </menulist>
              
              <checkbox id="quickquote_number_clear" checked="false"
                        label="番号で引用する時にコメント欄をクリア" />
              <checkbox id="quickquote_number_nocomment" checked="false"
                        label="本文なしの場合に番号にする" />
              <checkbox id="quickquote_number_onlyquote" checked="false"
                        class="sp" tooptiptext="(デフォルト：無効) メッセージ本文が引用文のみの場合にNo.+番号を引用する"
                        label="メッセージが引用のみの場合に番号にする" />
              <checkbox id="quickquote_number_nomenu" checked="false"
                        class="sp" tooptiptext="(デフォルト：無効) ふたばの引用・削除メニュー(2019/11月~)を表示させずに即座に引用する。ただしクリック時にAltキーShiftキー等を押しているとメニューが表示される。"
                        label="通常クリック時に引用・削除メニューを表示させずに引用" />
              <box class="sub" orient="vertical">
                <label value="※Alt,Shift等の修飾キー押し下げ時は逆の挙動になります" class="warning" />
              </box>
            </box>
            <checkbox id="quickquote_clear" checked="false"
                      label="メニューから引用する時にコメント欄をクリア" />
            <checkbox id="quickquote_untroll" checked="false"
                      label="引用時に芝刈りを解除する" />
            <checkbox id="quickquote_focus" checked="false"
                      label="検索したタブを選択する" />
          </box>
        </groupbox>
        <groupbox orient="vertical">
          <caption>
            <checkbox id="jpeg_thumbnail" checked="false"
                      label="JPEG のサムネを見る"
                      oncommand="AkahukuOptions.checkJPEGThumbnail ();" />
          </caption>
          <checkbox id="jpeg_thumbnail_error" checked="false"
                    label="見付からなかった場合にエラーを表示する" />
        </groupbox>
      </box>
      <box flex="1" id="tab_savemht" orient="vertical">
        <groupbox orient="vertical">
          <caption>
            <checkbox id="savemht" checked="false" label="[MHT で保存] ボタン"
                      oncommand="AkahukuOptions.checkSaveMHT ();" />
          </caption>
          <label value="UnMHT の mht で保存とは全く別のものです" class="warning" />
          <box orient="horizontal" align="center">
            <label id="savemht_base_label" control="savemht_base"
                   value="保存先のディレクトリ" />
            <textbox id="savemht_base" flex="1" />
            <button id="savemht_base_select" label="変更"
                    oncommand="AkahukuOptions.selectFile ('savemht_base', false, '保存先のディレクトリを選んでください');" />
          </box>
          <radiogroup id="savemht_default_type" orient="horizontal"
                      align="center"
                      oncommand="AkahukuOptions.checkSaveMHTDefaultType ();">
            <label id="savemht_default_label" control="savemht_default_type"
                   value="デフォルトのファイル名: " />
            <radio value="simple" label="簡易設定" />
            <radio value="expert" label="詳細設定" />
            <spacer flex="1" />
            <button id="savemht_default_format_init" label="初期化"
                    oncommand="AkahukuOptions.initSaveMHTDefaultFormat ();" />
          </radiogroup>
          <deck id="savemht_default_type_deck">
            <box id="savemht_default_type_deck_simple" orient="vertical"
                 flex="1">
              <checkbox id="savemht_default_server" checked="false"
                        label="サーバ名" />
              <checkbox id="savemht_default_dir" checked="false"
                        label="ディレクトリ名" />
              <checkbox id="savemht_default_thread" checked="false"
