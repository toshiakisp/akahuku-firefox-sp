            <checkbox id="sound_savemht" checked="false"
                      label="mht で保存" />
            <textbox id="sound_savemht_file" flex="1" />
            <button id="sound_savemht_file_select" label="変更"
                    oncommand="AkahukuOptions.selectFile ('sound_savemht_file', true, 'サウンドを選んでください');" />
            <button id="sound_savemht_file_play" label="再生"
                    oncommand="AkahukuOptions.playFile ('sound_savemht_file')" />
          </box>
          <box orient="horizontal" align="center">
            <checkbox id="sound_savemht_error" checked="false"
                      label="mht で保存失敗" />
            <textbox id="sound_savemht_error_file" flex="1" />
            <button id="sound_savemht_error_file_select" label="変更"
                    oncommand="AkahukuOptions.selectFile ('sound_savemht_error_file', true, 'サウンドを選んでください');" />
            <button id="sound_savemht_error_file_play" label="再生"
                    oncommand="AkahukuOptions.playFile ('sound_savemht_error_file')" />
          </box>
          <box orient="horizontal" align="center">
            <checkbox id="sound_saveimage" checked="false"
                      label="画像を保存" />
            <textbox id="sound_saveimage_file" flex="1" />
            <button id="sound_saveimage_file_select" label="変更"
                    oncommand="AkahukuOptions.selectFile ('sound_saveimage_file', true, 'サウンドを選んでください');" />
            <button id="sound_saveimage_file_play" label="再生"
                    oncommand="AkahukuOptions.playFile ('sound_saveimage_file')" />
          </box>
          <box orient="horizontal" align="center">
            <checkbox id="sound_saveimage_error" checked="false"
                      label="画像を保存失敗" />
            <textbox id="sound_saveimage_error_file" flex="1" />
            <button id="sound_saveimage_error_file_select" label="変更"
                    oncommand="AkahukuOptions.selectFile ('sound_saveimage_error_file', true, 'サウンドを選んでください');" />
            <button id="sound_saveimage_file_error_play" label="再生"
                    oncommand="AkahukuOptions.playFile ('sound_saveimage_error_file')" />
          </box>
        </groupbox>
      </box>
      <box flex="1" id="tab_other" orient="vertical">
        <groupbox orient="vertical">
          <caption label="高速化" />
          <checkbox id="add_checkbox_id" checked="false"
                    label="チェックボックスに id を付ける" />
        </groupbox>
        <groupbox orient="vertical">
          <caption label="ステータスバー" />
          <checkbox id="statusbar_preferences" checked="false"
                    label="設定ダイアログのアイコンを表示する" />
        </groupbox>
        <groupbox orient="vertical">
          <caption label="ツールバー" />
          <label control="toolbar_preferences"
                 value="Firefox では、ツールバーのカスタマイズから追加してください" class="warning" />
          <checkbox id="toolbar_preferences" checked="false"
                    label="設定ダイアログのアイコンを表示する" />
        </groupbox>
        <groupbox orient="vertical">
          <caption>
            <checkbox id="statusbar_order" checked="false"
                      label="ステータスバーのアイコンの並べ替え"
                      oncommand="AkahukuOptions.checkStatusbarOrder ();" />
          </caption>
          <box orient="horizontal">
            <listbox id="statusbar_order_list" flex="1"
                     rows="6" seltype="multiple">
            </listbox>
            <box orient="vertical">
              <button id="statusbar_order_moveup" label="上に"
                      oncommand="AkahukuOptions.onMoveupList ('statusbar_order_list');" />
              <button id="statusbar_order_movedown" label="下に"
                      oncommand="AkahukuOptions.onMovedownList ('statusbar_order_list');" />
            </box>
          </box>
        </groupbox>
        <groupbox orient="vertical">
          <caption label="設定のインポート／エクスポート／初期化" />
          <box orient="horizontal" flex="1">
            <button id="import" label="インポート"
                    oncommand="AkahukuOptions.importPrefs ();" />
            <button id="export" label="エクスポート"
                    oncommand="AkahukuOptions.exportPrefs (false);" />
            <button id="export2" label="エクスポート [個人情報削除]"
                    oncommand="AkahukuOptions.exportPrefs (true);" />
            <button id="init" label="初期化"
                    oncommand="AkahukuOptions.initPrefs ();" />
          </box>
          <label control="export"
                 value="mht で保存、画像を保存、P2P、ブルマ女将、音の設定は個人情報に繋がる可能性があります" class="warning" />
          <label control="export"
                 value="他人に公開する場合は [個人情報削除] の方でエクスポートしてください" class="warning" />
          <label id="import_export_message" value="" />
        </groupbox>
      </box>
      <box flex="1" id="tab_filename_convert" orient="vertical">
        <groupbox orient="horizontal" flex="1">
          <caption label="ファイル名変換" />
          <box orient="vertical" flex="1">
            <box orient="horizontal" align="center">
              <textbox id="filename_convert_from" flex="1" />
              <label id="filename_convert_label" control="filename_convert_to"
                     value="→" />
              <textbox id="filename_convert_to" flex="1" />
            </box>
            <label id="filename_convert_illegal" value="" class="warning" />
            <listbox id="filename_convert_list" flex="1"
                     rows="6" seltype="multiple">
              <listhead>
                <listheader label="変換元" />
                <listheader label="変換先" />
                <listheader label="" />
              </listhead>
              <listcols>
                <listcol style="min-width: 8em;" flex="1" />
                <listcol style="min-width: 8em;" flex="1" />
                <listcol />
              </listcols>
            </listbox>
          </box>
          <box orient="vertical">
            <button id="filename_convert_add" label="追加" />
            <button id="filename_convert_modify" label="修正" />
            <button id="filename_convert_moveup" label="上に" />
            <button id="filename_convert_movedown" label="下に" />
            <button id="filename_convert_delete" label="削除" />
            <button id="filename_convert_init" label="初期化"
                    oncommand="AkahukuOptions.initFilenameConvert ();" />
          </box>
        </groupbox>
      </box>
      <box flex="1" id="tab_format" orient="vertical">
        <groupbox orient="vertical">
          <caption label="詳細設定のフォーマット中で使える実体参照" />
          <grid>
            <columns>
              <column flex="1">
                <label value="&amp;YY; 年" />
                <label value="&amp;MM; 月" />
                <label value="&amp;DD; 日" />
                <label value="&amp;DDD; 曜日" />
                <label value="&amp;hh; 時" />
                <label value="&amp;mm; 分" />
                <label value="&amp;ss; 秒" />
                <label value="&amp;board; 板名" />
                <label value="&amp;board2; サーバ名無しの板名" />
                <label value="&amp;board3; 本来の板名" />
                <label value="&amp;server; サーバ名" />
                <label value="&amp;dir; ディレクトリ名" />
                <label value="&amp;page; ページ番号" />
                <label value="&amp;thread; スレの番号" />
                <label value="&amp;replycount; レス数" />
                <label value="&amp;url; URL ([画像を保存] のみ)" />
              </column>
              <column flex="1">
                <label value="&amp;message; 本文の最初" />
                <label value="&amp;message2; 本来の本文の最初" />
                <label value="&amp;entiremessage; 本文全体" />
                <label value="&amp;name; 名前" />
                <label value="&amp;mail; メル欄" />
                <label value="&amp;subject; 題" />
                <label value="&amp;ip; IP アドレス" />
                <label value="&amp;id; ID" />
                <label value="&amp;viewer; 見ている人数" />
                <label value="&amp;expire; 消滅時刻" />
                <label value="&amp;warning; 消滅情報" />
                <label value="&amp;separator; ディレクトリの区切り" />
                <label value="&amp;amp; 「&amp;」" />
                <label value="&amp;lt; 「&lt;」" />
                <label value="&amp;gt; 「&gt;」" />
              </column>
            </columns>
          </grid>
        </groupbox>
      </box>
    </deck>
  </box>
</dialog>
